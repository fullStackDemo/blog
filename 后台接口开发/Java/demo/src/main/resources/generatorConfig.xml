<generatorConfiguration>

    <classPathEntry location="/Users/wangzhao/.m2/repository/mysql/mysql-connector-java/8.0.16/mysql-connector-java-8.0.16.jar"/>

    <context id="mysql" targetRuntime="MyBatis3Simple">
